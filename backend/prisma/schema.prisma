generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  username String @unique @db.VarChar(255)
  email    String @db.VarChar(255)
  password String @db.VarChar(255)

  @@map("users")
}
